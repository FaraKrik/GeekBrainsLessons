<?xml version="1.0" encoding="utf-8"?>
<androidx.constraintlayout.widget.ConstraintLayout xmlns:android="http://schemas.android.com/apk/res/android"
    xmlns:app="http://schemas.android.com/apk/res-auto"
    xmlns:tools="http://schemas.android.com/tools"
    android:layout_width="match_parent"
    android:layout_height="match_parent"
    android:soundEffectsEnabled="false"
    tools:context=".MainActivity">

    <LinearLayout
        android:id="@+id/linearLayout"
        android:layout_width="391dp"
        android:layout_height="783dp"
        android:orientation="horizontal"
        app:layout_constraintEnd_toStartOf="parent"
        tools:layout_editor_absoluteY="1dp">

        <EditText
            android:id="@+id/editTextTextEmailAddress"
            android:layout_width="wrap_content"
            android:layout_height="wrap_content"
            android:layout_weight="1"
            android:ems="10"
            android:inputType="textEmailAddress" />
    </LinearLayout>

    <com.google.android.material.textfield.TextInputLayout
        android:layout_width="408dp"
        android:layout_height="wrap_content"
        app:layout_constraintBottom_toBottomOf="parent"
        app:layout_constraintTop_toTopOf="parent"
        tools:layout_editor_absoluteX="2dp">

    </com.google.android.material.textfield.TextInputLayout>

    <com.google.android.material.textfield.TextInputEditText
        android:layout_width="409dp"
        android:layout_height="50dp"
        android:height="0"
        android:gravity="center"
        android:hint="New object"
        android:hyphenationFrequency="normal"
        tools:layout_editor_absoluteX="-1dp"
        tools:layout_editor_absoluteY="12dp" />

    <TextView
        android:id="@+id/textView"
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:text="Request"
        tools:layout_editor_absoluteX="190dp"
        tools:layout_editor_absoluteY="79dp" />

    <EditText
        android:id="@+id/editTextPhone"
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:autoLink="phone"
        android:ems="11"
        android:inputType="phone"
        android:text="             8812999999"
        tools:layout_editor_absoluteX="100dp"
        tools:layout_editor_absoluteY="115dp" />

    <EditText
        android:id="@+id/editTextNumber4"
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:backgroundTint="#000000"
        android:drawableTint="#250101"
        android:ems="10"
        android:foregroundTint="#000000"
        android:hint="Number #9329329"
        android:hyphenationFrequency="normal"
        android:inputType="number"
        android:password="false"
        tools:layout_editor_absoluteX="100dp"
        tools:layout_editor_absoluteY="189dp" />

    <EditText
        android:id="@+id/editTextTextEmailAddress2"
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:autoLink="email"
        android:ems="10"
        android:inputType="textEmailAddress"
        android:scrollbarThumbHorizontal="?attr/actionModeShareDrawable"
        android:singleLine="false"
        android:text="Введите e-mail"
        tools:layout_editor_absoluteX="100dp"
        tools:layout_editor_absoluteY="310dp" />

    <CalendarView
        android:id="@+id/calendarView"
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:layout_marginTop="376dp"
        android:importantForAccessibility="auto"
        android:isScrollContainer="false"
        android:longClickable="true"
        android:measureAllChildren="true"
        app:layout_constraintTop_toTopOf="parent"
        tools:layout_editor_absoluteX="30dp" />

</androidx.constraintlayout.widget.ConstraintLayout>